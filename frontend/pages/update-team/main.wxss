.search-list-container {
  margin-top: 0%;
  left: 0;
  right: 0;
  background-color: #fff;
  border: 2rpx solid #ddd;
  border-radius: 8rpx;
  max-height: 200rpx;
  overflow-y: auto;
  position: absolute;
  z-index: 1;
}
.search-list {
  max-height: 300rpx;
}
.search-list-item {
  padding: 10rpx;
  border-bottom: 2rpx solid #f0f0f0;
}
.allview {
  display: flex;
